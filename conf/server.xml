<?xml version="1.0" encoding="UTF-8"?>

<server>
  <interpreter>
    <interpreter-name>DEFAULT</interpreter-name>
    <interpreter-class>org.safehaus.penrose.interpreter.DefaultInterpreter</interpreter-class>
  </interpreter>
  <engine>
    <engine-name>DEFAULT</engine-name>
    <engine-class>org.safehaus.penrose.engine.Engine</engine-class>
    <cache>
      <cache-name>Data Cache</cache-name>
      <cache-class>org.safehaus.penrose.cache.DefaultEngineDataCache</cache-class>
    </cache>
    <cache>
      <cache-name>Query Cache</cache-name>
      <cache-class>org.safehaus.penrose.cache.DefaultEngineQueryCache</cache-class>
    </cache>
  </engine>
  <connector>
    <connector-name>DEFAULT</connector-name>
    <connector-class>org.safehaus.penrose.connector.Connector</connector-class>
    <cache>
      <cache-name>Data Cache</cache-name>
      <cache-class>org.safehaus.penrose.cache.DefaultConnectorDataCache</cache-class>
    </cache>
    <cache>
      <cache-name>Query Cache</cache-name>
      <cache-class>org.safehaus.penrose.cache.DefaultConnectorQueryCache</cache-class>
    </cache>
  </connector>
  <adapter>
    <adapter-name>JDBC</adapter-name>
    <adapter-class>org.safehaus.penrose.connection.JDBCAdapter</adapter-class>
  </adapter>
  <adapter>
    <adapter-name>JNDI</adapter-name>
    <adapter-class>org.safehaus.penrose.connection.JNDIAdapter</adapter-class>
  </adapter>
</server>
