<?xml version="1.0" encoding="UTF-8"?>
<!--
<!DOCTYPE mapping PUBLIC
	"-//Penrose/Penrose Server Configuration DTD 1.0//EN"
	"http://penrose.safehaus.org/dtd/penrose-mapping-config-1.0.dtd">
-->

<mapping>

  <entry dn="dc=Penrose,dc=Safehaus,dc=org">
    <oc>dcObject</oc>
    <oc>organization</oc>
    <at name="dc" rdn="true">
      <constant>Penrose</constant>
    </at>
    <at name="o">
      <constant>Penrose</constant>
    </at>
    <aci>
      <permission>rs</permission>
    </aci>
  </entry>

  <entry dn="cn=Manager,dc=Penrose,dc=Safehaus,dc=org">
    <oc>person</oc>
    <oc>organizationalPerson</oc>
    <oc>inetOrgPerson</oc>
    <at name="cn" rdn="true">
      <constant>Manager</constant>
    </at>
    <at name="sn">
      <constant>Manager</constant>
    </at>
    <at name="userPassword">
      <constant>secret</constant>
    </at>
  </entry>

  <entry dn="ou=Users,dc=Penrose,dc=Safehaus,dc=org">
    <oc>organizationalUnit</oc>
    <at name="ou" rdn="true">
      <constant>Users</constant>
    </at>
  </entry>

  <entry dn="ou=Groups,dc=Penrose,dc=Safehaus,dc=org">
    <oc>organizationalUnit</oc>
    <at name="ou" rdn="true">
      <constant>Groups</constant>
    </at>
  </entry>

</mapping>
