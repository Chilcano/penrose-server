<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE modules PUBLIC
  "-//Penrose/DTD Modules ${product.specification}//EN"
  "http://penrose.safehaus.org/dtd/modules.dtd">

<modules>

  <module name="NISLDAPSyncModule" enabled="true">
    <module-class>org.safehaus.penrose.nis.module.NISLDAPSyncModule</module-class>
    <parameter>
      <param-name>source</param-name>
      <param-value>${DOMAIN}_yp</param-value>
    </parameter>
<!--
    <parameter>
      <param-name>changes</param-name>
      <param-value>changes</param-value>
    </parameter>
-->
    <parameter>
      <param-name>errors</param-name>
      <param-value>errors</param-value>
    </parameter>
  </module>

  <module name="LinkingModule" enabled="true">
    <module-class>org.safehaus.penrose.federation.module.LinkingModule</module-class>
    <parameter>
      <param-name>target</param-name>
      <param-value>global</param-value>
    </parameter>
    <parameter>
      <param-name>sourceAttribute</param-name>
      <param-value>${linkingLocalAttribute}</param-value>
    </parameter>
    <parameter>
      <param-name>targetAttribute</param-name>
      <param-value>${linkingGlobalAttribute}</param-value>
    </parameter>
    <parameter>
      <param-name>storage</param-name>
      <param-value>${linkingStorage}</param-value>
    </parameter>
    <parameter>
      <param-name>mapping</param-name>
      <param-value>${importMappingName}</param-value>
    </parameter>
    <parameter>
      <param-name>mappingPrefix</param-name>
      <param-value>${importMappingPrefix}</param-value>
    </parameter>
  </module>

</modules>